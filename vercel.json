{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json", 
        "use": "@vercel/static-build" 
      },
      {
        "src": "server.js", 
        "use": "@vercel/node" 
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/client/build/$1" 
      },
      {
        "src": "/api/(.*)",
        "dest": "/server.js" 
      }
    ]
  }
  